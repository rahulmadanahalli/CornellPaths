al papers."}],"campus":"MAIN","campusDescr":"Ithaca","location":"ITH","locationDescr":"Ithaca, NY (Main Campus)","startDt":"01/25/2017","endDt":"05/10/2017","addConsent":"N","addConsentDescr":"No Special Consent Required","isComponentGraded":true,"instructionMode":"P","instrModeDescrshort":"In Person","instrModeDescr":"In Person","topicDescription":"Didactic Poetry"}],"unitsMinimum":4,"unitsMaximum":4,"componentsOptional":[],"componentsRequired":["SEM"],"gradingBasis":"OPT","gradingBasisShort":"Stdnt Opt","gradingBasisLong":"Letter or S/U grades","simpleCombinations":[],"sessionCode":"1","sessionBeginDt":"01/25/2017","sessionEndDt":"05/10/2017","sessionLong":"Regular Academic Session"}],"description":"A graduate seminar in Latin.","catalogBreadth":"","catalogDistr":"","catalogLang":"","catalogForbiddenOverlaps":"","catalogAttribute":"","catalogWhenOffered":"Spring.","catalogComments":"","catalogPrereqCoreq":"","catalogFee":"","catalogSatisfiesReq":"","catalogPermission":"Enrollment limited to: graduate students.","catalogCourseSubfield":"","catalogOutcomes":null,"acadCareer":"GR","acadGroup":"AS"},{"strm":2657,"crseId":354488,"crseOfferNbr":1,"subject":"LATIN","catalogNbr":"7920","titleShort":"Independ Study In Latin","titleLong":"Independent Study in Latin","enrollGroups":[{"classSections":[{"ssrComponent":"IND","ssrComponentLong":"Independent Study","section":"601","classNbr":5858,"meetings":[{"classMtgNbr":1,"timeStart":"","timeEnd":"","startDt":"01/25/2017","endDt":"05/10/2017","instructors":[],"pattern":"","facilityDescr":null,"bldgDescr":null,"facilityDescrshort":null,"meetingTopicDescription":""}],"notes":[],"campus":"MAIN","campusDescr":"Ithaca","location":"ITH","locationDescr":"Ithaca, NY (Main Campus)","startDt":"01/25/2017","endDt":"05/10/2017","addConsent":"D","addConsentDescr":"Department Consent Required","isComponentGraded":true,"instructionMode":"P","instrModeDescrshort":"In Person","instrModeDescr":"In Person","topicDescription":""}],"unitsMinimum":